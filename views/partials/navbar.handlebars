<nav class="navbar navbar-expand-md fixed-top navbar-dark" style="background-color: #030089;">

    <ul class="navbar-nav mr-auto">
        <li class="nav-item dropdown active">
            <a class="nav-link navbar-brand dropdown-toggle" data-toggle="dropdown" href="#">
                {{#if course}}
                    {{course.number_upper}}
                {{else}}
                    CSE
                {{/if}}
            </a>
            <div class="dropdown-menu">
                {{#each course_list}}
                    {{#unless this.archived}}
                        <a class="dropdown-item" href="/courses/{{this.course}}/syllabus">{{this.number_upper}}
                            : {{this.title}}</a>
                    {{/unless}}
                {{/each}}
                <a class="dropdown-item" href="/courses/">Archive</a>
            </div>
        </li>

    </ul>

    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>


    <!--<ul class="navbar-nav mr-auto">-->

    <!--</ul>-->

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto">


            {{#if course.assignments}}
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="/courses/{{course.course}}/">
                        <div class="display-text">Assessment</div>
                        <div class="tiny-text">A<span class="glyphicon glyphicon-star" aria-hidden="true"></span>
                        </div>
                    </a>
                    <div class="dropdown-menu">

                        {{#unless course.course_options.no_autolab}}
                            <a class="dropdown-item" href="https://autograder.cse.buffalo.edu/" target="_blank">
                                Autolab</a>
                        {{/unless}}
                        {{#each course.assignments}}

                            <a class="dropdown-item"
                               href="/courses/{{../course.course}}/assignments/{{this.short_title}}">{{this.title}}</a>

                        {{/each}}
                    </div>
                </li>
            {{/if}}


            {{#if course.lectures}}
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="/courses/{{course.course}}/">
                        <div class="display-text">Training</div>
                        <div class="tiny-text">T<span class="glyphicon glyphicon-book" aria-hidden="true"></span>
                        </div>

                    </a>
                    <div class="dropdown-menu">
                        {{#unless course.course_options.no_question_site}}
                            <a class="dropdown-item" href="https://fury.cse.buffalo.edu/questions/index.php"
                               target="_blank">
                                Questions</a>
                        {{/unless}}
                        {{#each course.lectures}}
                            <a class="dropdown-item"
                               href="/courses/{{../course.course}}/lectures/{{this.short_title}}">{{this.title}}</a>
                        {{/each}}
                    </div>
                </li>
            {{/if}}

            {{#each course.extra}}
                <li class="nav-item"><a class="nav-link"
                                        href="/courses/{{../course.course}}/{{this.page_type}}">{{this.title}}</a></li>
            {{/each}}
        </ul>


        <ul class="navbar-nav navbar-right">
            <!--<li><a href="#"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>-->
            {{#if user}}
                {{#if course}}
                    {{#ifNotExists user.courses_enrolled course.course}}
                        <a class="nav-link" href="/courses/{{course.course}}/enroll">Enroll</a>
                    {{/ifNotExists}}
                {{/if}}
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="/user/profile">
                        {{user.username}}<span class="caret"></span>
                    </a>
                    <div class="dropdown-menu">
                        <a class="dropdown-item" href="/user/profile">Profile</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="/user/logout">Logout</a>
                    </div>
                </li>
            {{else}}

                {{#if course.course_options.student_account}}
                    <a href="/user/login">Login</a>
                {{/if}}
            {{/if}}
        </ul>


        <!--<form class="form-inline my-2 my-lg-0">-->
